{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "#root/*": [
                "./*"
            ],
            "#src/*": [
                "src/*"
            ],
            "#routes/*": [
                "src/routes/*"
            ],
            "#models/*": [
                "src/models/*"
            ],
            "#middlewares/*": [
                "src/middlewares/*"
            ],
            "#helpers/*": [
                "src/helpers/*"
            ],
            "#controllers/*": [
                "src/controllers/*"
            ],
            "#config/*": [
                "src/config/*"
            ],
        }
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.js",
        "src/**/*.jsx",
        "src/**/*.vue"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "build",
        "public"
    ]
}